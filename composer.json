{
    "name": "jmrashed/logger",
    "description": "A lightweight, production-ready logging utility for development environments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Rasheduzzzaman",
            "email": "jmrashed@gmail.com",
            "homepage": "https://github.com/jmrashed/logger"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DevLogger\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevLogger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}